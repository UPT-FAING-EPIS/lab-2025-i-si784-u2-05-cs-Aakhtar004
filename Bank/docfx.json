{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "**/Bank.Domain.csproj"
          ]
        }
      ],
      "dest": "../api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "../api/**.yml",
          "../api/index.md"
        ]
      },
      {
        "files": [
          "../*.md",
          "../toc.yml"
        ]
      }
    ],
    "dest": "../_site",
    "globalMetadata": {
      "_appName": "Bank Domain Documentation",
      "_appTitle": "Bank Domain Documentation"
    },
    "template": [
      "default"
    ]
  }
}
